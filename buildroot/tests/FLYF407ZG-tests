#!/usr/bin/env bash
#
# Build tests for FLYF407ZG
#

# exit on first failure
set -e

# Build examples
restore_configs
opt_set MOTHERBOARD BOARD_FLYF407Z SERIAL_PORT - X_DRIVER_TYPE TMC220 Y_DRIVER_TYPE TMC2130
exec_test $1 $2 "FLYF407ZG Default Config with mixed TMC Drivers" "$3"

# cleanup
restore_configs
